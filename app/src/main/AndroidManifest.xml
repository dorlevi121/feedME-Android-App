<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dor.testfeedme">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="MainActivity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="Register.signUp" />
        <activity android:name="Register.EntrySurveyText" />
        <activity android:name="Register.ResetPassword" />
        <activity android:name="FeedMeButton.GenerateSuggestionsActivity" />
        <activity android:name="FeedMeButton.ShowRecipeActivity" />
        <activity android:name="Users.GuestActivity"></activity>
    </application>

</manifest>